{
  "mcpServers": {
    "jakes-dad": {
      "command": "uv",
      "args": [
        "--directory",
        "apps/mcp-server",
        "run",
        "jakes-dad-mcp"
      ],
      "env": {
        "SUPABASE_URL": "${SUPABASE_URL}",
        "SUPABASE_SERVICE_KEY": "${SUPABASE_SERVICE_KEY}"
      }
    }
  }
}
